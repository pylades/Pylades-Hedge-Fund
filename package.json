{
  "name": "blackpool.finance-client",
  "version": "1.0.0",
  "scripts": {
    "start": "next start",
    "dev": "next",
    "build": "npm run typecheck && npm run prettier && npm run build:theme-prod && npm run build:app",
    "build:app": "next build",
    "build:theme-dev": "tailwind build styles/builder.css > styles/theme.css",
    "build:theme-prod": "cross-env NODE_ENV=production tailwind build styles/builder.css > styles/theme.css",
    "prettier": "prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "humps": "^2.0.1",
    "next": "^10.0.3",
    "next-images": "^1.6.2",
    "nprogress": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.14",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^9",
    "cross-env": "^7.0.3",
    "postcss": "^7",
    "prettier": "^2.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.3"
  },
  "license": "ISC"
